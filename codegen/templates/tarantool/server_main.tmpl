{{- define "server_main" -}}
-- DO NOT EDIT THIS FILE. This file will be overwritten when re-running go-raml.
{{- range $k, $v := .Resources }}
local {{.Name | ToLower}}_api = require("{{.Name | ToLower}}_api")
{{- end }}

httpd = require('http.server').new("0.0.0.0", 5000)
{{ range $k, $v := .Resources }}
{{.Name | ToLower}}_api(httpd)
{{- end }}

httpd:start()
{{- end -}}
