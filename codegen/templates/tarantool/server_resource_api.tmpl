{{- define "server_resource_api" -}}
-- DO NOT EDIT THIS FILE. This file will be overwritten when re-running go-raml.

local handlers = require("handlers.handlers")

function init(httpd)
{{- range $k, $v := .Methods }}
    httpd:route({ path = '{{$v.URI}}', method = '{{$v.Verb}}'}, handlers.{{$v.Handler}})
{{- end}}
end

return init
{{- end -}}


